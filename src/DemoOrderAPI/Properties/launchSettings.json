{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "DemoOrderAPI": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "Orders",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "applicationUrl": "http://localhost:5001"
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/Orders",
      "publishAllPorts": true,
      "useSSL": true
    }
  }
}